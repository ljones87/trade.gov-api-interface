    "react": "^15.6.1",
    "react-data-export": "^0.4.2",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.5",
    "react-router-dom": "^4.1.2",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "sequelize": "^4.2.1",
    "victory": "^0.21.3"
  },
